# Test that unescaped text which is present is output 

%TEMPLATE_MODE HTML5

%CONTEXT
value = "<b>Something</b>"

%INPUT
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ifnotnull="http://www.ultraq.net.nz/thymeleaf/ifnotnull">
	<body>
		<p ifnotnull:utext="${value}"></p>
	</body>
</html>

%OUTPUT
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
	<body>
		<p><b>Something</b></p>
	</body>
</html>
